class Solution(object):
    def canConstruct(self, ransomNote, magazine):
        """
        :type ransomNote: str
        :type magazine: str
        :rtype: bool
        """
        seen = {}

        for char in magazine:
            if char not in seen:
                seen[char] = 1
            else:
                seen[char] +=1
        
        for x in ransomNote:
            if x in seen and seen[x] > 0:
                seen[x] -=1 
            else:
                return False
        return True
