# Definition for a binary tree node.
# class TreeNode(object):
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution(object):
    def inorderTraversal(self, root):
        """
        :type root: Optional[TreeNode]
        :rtype: List[int]
        """
        output = []
        def recurse(node):
            if not node:
                return
            recurse(node.left)
            output.append(node.val)
            recurse(node.right)      
        recurse(root)
        return output
