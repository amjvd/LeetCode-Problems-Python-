class Solution(object):
    def maximum69Number (self, num):
        """
        :type num: int
        :rtype: int
        """

        digits = list(str(num))
        if num > 0:
            for x in range(len(digits)):
                if digits[x] == "6":
                    digits[x] = "9"
                    break
            return int(''.join(digits))
