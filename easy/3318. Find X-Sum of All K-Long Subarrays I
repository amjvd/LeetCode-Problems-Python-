class Solution(object):
    def findXSum(self, nums, k, x):
        """
        :type nums: List[int]
        :type k: int
        :type x: int
        :rtype: List[int]
        """
        output = []
        n = len(nums)

        for i in range(0,n-k+1):
            current = nums[i:k+i]
            freq = Counter(current)
            sortedFreq = sorted(freq.items(), key = lambda a:(-a[1], -a[0]))
            topx = set(num for num, _ in sortedFreq[:x])
            xsum = sum(num for num in current if num in topx)
            output.append(xsum)

        return output
