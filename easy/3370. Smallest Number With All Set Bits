class Solution(object):
    def smallestNumber(self, n):
        """
        :type n: int
        :rtype: int
        """
        """
        nums = [1, 3, 7, 15, 31, 63, 127, 255, 511, 1023]

        for x in nums:
            if x >= n:
                return x
        """
        
        num = bin(n)
        output = num[2:].replace('0', '1')
        output = int(output, 2)
        return output
