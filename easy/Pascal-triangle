class Solution(object):
    def generate(self, numRows):
        """
        :type numRows: int
        :rtype: List[List[int]]
        """
        
        pascal = []

        for i in range(numRows):
            row = [1] * (i + 1)    #row with right amount of numbers for pascals triangle (all set to 1 for now)
            for j in range(1, i):
                row[j] = pascal[i - 1][j - 1] + pascal[i - 1][j]   #goes through row to add needed ones.
            pascal.append(row)
        
        return pascal
"""
problem number 118
"""
