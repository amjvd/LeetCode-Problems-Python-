class Solution(object):
    def productExceptSelf(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """

        res = [1] * len(nums)
        left = 1

        for x in range(len(nums)):
            res[x] *= left
            left *= nums[x]
        
        right = 1
        
        for x in range(len(nums)-1, -1, -1):
            res[x] *= right
            right *= nums[x]
        return res
