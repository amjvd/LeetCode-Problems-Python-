class Solution(object):
    def sortVowels(self, s):
        """
        :type s: str
        :rtype: str
        """
        vowels = ["A", "E", "I", "O", "U", "a", "e", "i", "o", "u"]
        found = []
        pos = []

        for x in range(len(s)):
            if s[x] in vowels:
                found.append(s[x])
                pos.append(x)
        
        found.sort()
        slist = list(s)
        for x in pos:
            for y in range(len(found)):
                slist[x] = found[y]
        return slist
